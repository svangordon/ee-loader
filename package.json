{
  "name": "ee-loader",
  "version": "0.5.0",
  "description": "Builds the code at src/index.js and serves it to the Earthe Engine IDE.",
  "bin": {
    "ee-loader": "./server.js"
  },
  "config": {
    "defaultEntry": "src/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/index.js",
    "chrome": "google-chrome https://code.earthengine.google.com --new-window --allow-insecure-localhost",
    "start": "node server.js $1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svangordon/ee-loader"
  },
  "author": "Stephen Van Gordon",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "webpack": "^3.3.0"
  },
  "keywords": [
    "earth engine",
    "webpack",
    "javascript"
  ]
}
